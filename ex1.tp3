#include<stdio.h>
int main()
{
   
    int n,i;
    int resultat = 1;
           printf("Entrez un nombre positif ou nul pour connaitre son factorielle : \n");
           scanf("%d", &n);
  for (i=2 ; i<=n ; i++)
        resultat *= i;
 
            printf("La factorielle de %d est : %d", n, resultat);
 
    return 0;
}
